{
  "name": "my-app",
  "version": "0.1.0",
  "private": true,
  "main": "src/index.tsx",
  "dependencies": {
    "apollo-boost": "0.1.16",
    "apollo-client": "^2.3.8",
    "babel-core": "6",
    "babel-runtime": "^6.23.0",
    "graphql": "^0.13.0",
    "graphql-tag": "2.10.0",
    "react": "^16.2.0",
    "react-apollo": "2.2.4",
    "react-dom": "^16.2.0"
  },
  "scripts": {
    "codegen:generate": "apollo codegen:generate --outputFlat --schema=schema.json --target=typescript src/types",
    "schema:download": "apollo schema:download --endpoint=https://api.newrelic.com/graphql --header=\"API-KEY: $REACT_APP_API_KEY\"",
    "start": "react-scripts-ts start",
    "build": "react-scripts-ts build",
    "test": "react-scripts-ts test --env=jsdom",
    "eject": "react-scripts-ts eject"
  },
  "devDependencies": {
    "@types/graphql": "14.0.1",
    "@types/jest": "^22.1.3",
    "@types/node": "^9.4.6",
    "@types/react": "^16.4.16",
    "@types/react-dom": "16.0.4",
    "apollo": "^1.9.2",
    "jest": "^23.6.0",
    "react-scripts-ts": "^2",
    "typescript": "^2"
  }
}
